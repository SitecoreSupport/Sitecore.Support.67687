<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <pipelines>
      <processMessage>
        <processor type="Sitecore.Form.Core.Pipelines.ProcessMessage.ProcessMessage, Sitecore.Forms.Core" method="ExpandTokens">
          <patch:attribute name="type">Sitecore.Support.Form.Core.Pipelines.ProcessMessage.ProcessMessage, Sitecore.Support.67687</patch:attribute>
        </processor>
      </processMessage>
      <exportToXml>
        <processor type="Sitecore.WFFM.Services.Pipelines.ExportToXml.ExportFormDataToXml, Sitecore.WFFM.Services">
          <patch:attribute name="type">Sitecore.Support.WFFM.Services.Pipelines.ExportToXml.ExportFormDataToXml, Sitecore.Support.67687</patch:attribute>
        </processor>
      </exportToXml>
      <exportToExcel>
        <processor type="Sitecore.WFFM.Services.Pipelines.ExportToExcel.ExportFormDataToExcel, Sitecore.WFFM.Services">
          <patch:attribute name="type">Sitecore.Support.WFFM.Services.Pipelines.ExportToExcel.ExportFormDataToExcel, Sitecore.Support.67687</patch:attribute>
        </processor>
      </exportToExcel>
    </pipelines>
    <settings>
      <!-- MESSAGE DATE FORMAT
           Set format for the Date and Date Picker fields that will be used in the email messages
      -->
      <setting name="WFM.MessageDateFormat" value="dd-MMM-yyyy" />
      <!-- EXPORT DATE FORMAT
           Set format for the Date and Date Picker fields that will be used in the exported Excel/XML files
      -->
      <setting name="WFM.ExportDateFormat" value="dd-MMM-yyyy" />
    </settings>
  </sitecore>
</configuration>